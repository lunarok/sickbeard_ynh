{
  "name": "Sickbeard",
  "id": "sickbeard",
  "description": {
    "en": "Web-based media streaming",
    "fr": "Diffuion audio-vid√©o en mode web "
  },
  "developer": {
    "name": "lunarok",
    "email": "cedric@wayang.fr",
    "url": "http://www.wayang.fr"
  },
  "multi_instance": "false",
  "arguments": {
    "install": [
      {
        "name": "domain",
        "ask": {
          "en": "Choose a domain for Sickbeard"
        },
        "example": "domain.org"
      },
      {
        "name": "path",
        "ask": {
          "en": "Choose a path for Sickbeard"
        },
        "example": "/series",
        "default": "/series"
      },
      {
        "name": "server",
        "ask": {
          "en": "Choose IP where Sickbeard is installed"
        },
        "example": "127.0.0.1",
        "default": "127.0.0.1"
      }
    ]
  }
}
